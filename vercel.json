{
    "builds": [{
        "src": "sports_management_system/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "sports_management_system/wsgi.py"
        }
    ]
}